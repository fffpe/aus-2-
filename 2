  <script>
  let form = document.querySelector('form');
let inputAffid = document.createElement('input');
inputAffid.type = 'hidden';
inputAffid.name = 'affid';
inputAffid.value = 197;
form.appendChild(inputAffid);
let inputFunnel = document.createElement('input');
inputFunnel.type = 'hidden';
inputFunnel.name = 'funnel';
inputFunnel.value = 'immediatefortune.com';
form.appendChild(inputFunnel);
let xhr = new XMLHttpRequest();
xhr.open('GET', 'https://jsonip.com/?callback=', true);
xhr.send();
xhr.onload = function () {
    let response = JSON.parse(xhr.responseText);
    let inputIp = document.createElement('input');
    inputIp.type = 'hidden';
    inputIp.name = '_ip';
    inputIp.value = response.ip;
    form.appendChild(inputIp);
}
form.addEventListener('submit',
    function (e) {
        e.preventDefault();
        let data = new FormData(form);
        console.log(data);
        let xhr = new XMLHttpRequest();
        xhr.open('POST', 'https://api.proapi.org/leads', true);
        xhr.send(data);
        xhr.onload = function () {
            let response = JSON.parse(xhr.responseText);
            if (response.errors) {
                alert(response.errors[0]);
            } else {
                // disable form
                form.style.display = 'none';
                document.querySelector('.w-form-done').style.display = 'block';
            }
        }
    }
);
</script>
